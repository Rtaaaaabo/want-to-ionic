{
  "name": "wanto",
  "version": "0.0.1",
  "author": "Taku Nakagawa",
  "homepage": "http://taku-cloud.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --silent=false --verbose false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "compodoc": "compodoc -p tsconfig.app.json src -s",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@aws-amplify/ui-angular": "^0.2.15",
    "@briebug/jest-schematic": "^3.1.0",
    "@capacitor/android": "^2.4.0",
    "@capacitor/core": "^2.4.5",
    "@capacitor/ios": "^2.4.0",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/date-picker": "^5.28.0",
    "@ionic-native/file": "^5.30.0",
    "@ionic-native/image-picker": "^5.28.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.6.2",
    "@ionic/pwa-elements": "^3.0.2",
    "@types/jest": "^26.0.8",
    "aws-amplify": "^3.3.3",
    "cordova-plugin-datepicker": "^0.9.3",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-telerik-imagepicker": "^2.3.5",
    "jest-preset-angular": "^8.4.0",
    "rxjs": "~6.5.1",
    "ts-jest": "^26.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.1.1",
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular/cli": "^11.2.5",
    "@angular/compiler": "~11.2.6",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/language-service": "~11.2.6",
    "@capacitor/cli": "^0.0.113",
    "@compodoc/compodoc": "^1.1.11",
    "@ionic/angular-toolkit": "^2.1.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "compodoc": "0.0.41",
    "graphql-auto-transformer": "^1.3.1",
    "jest": "^26.6.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "description": "For Collaboration"
}
